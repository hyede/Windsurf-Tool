{
  "name": "windsurf-tool",
  "version": "4.0.0",
  "description": "Windsurf-Tool - 自动化管理 Windsurf 账号，支持批量注册、账号切换、到期提醒等功能",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "build": "electron-builder",
    "build:all": "npm run build:mac && npm run build:win && npm run build:linux",
    "build:mac": "CSC_TIMESTAMP_SERVER= PATH=\"$HOME/bin:$PATH\" electron-builder --mac",
    "build:mac:x64": "CSC_TIMESTAMP_SERVER= PATH=\"$HOME/bin:$PATH\" electron-builder --mac --x64",
    "build:mac:arm64": "CSC_TIMESTAMP_SERVER= PATH=\"$HOME/bin:$PATH\" electron-builder --mac --arm64",
    "build:mac:interactive": "./build-mac.sh",
    "build:win": "electron-builder --win",
    "build:win:x64": "electron-builder --win --x64",
    "build:win:arm64": "electron-builder --win --arm64",
    "build:win:docker": "./build-docker.sh",
    "build:linux": "electron-builder --linux",
    "build:linux:x64": "electron-builder --linux --x64",
    "postinstall": "node -e \"const fs = require('fs'); const path = require('path'); const dir = path.join(__dirname, 'scripts'); if(fs.existsSync(dir)) { fs.readdirSync(dir).filter(f => f.endsWith('.py')).forEach(f => { const p = path.join(dir, f); if(process.platform !== 'win32') fs.chmodSync(p, 0o755); }); }\""
  },
  "keywords": [
    "windsurf",
    "account",
    "automation",
    "registration",
    "management"
  ],
  "author": "crispvibe",
  "license": "MIT",
  "homepage": "https://github.com/crispvibe/Windsurf-Tool",
  "repository": {
    "type": "git",
    "url": "https://github.com/crispvibe/Windsurf-Tool.git"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "chrome-launcher": "^1.2.1",
    "fs-extra": "^11.3.2",
    "imap": "^0.8.19",
    "lucide": "^0.553.0",
    "mailparser": "^3.6.5",
    "puppeteer": "^21.11.0",
    "puppeteer-real-browser": "^1.3.8",
    "sql.js": "^1.10.3",
    "uuid": "^9.0.1"
  },
  "optionalDependencies": {
    "robotjs": "^0.6.0"
  },
  "devDependencies": {
    "cross-env": "^10.1.0",
    "electron": "^26.6.10",
    "electron-builder": "^24.13.3",
    "javascript-obfuscator": "^4.1.1",
    "node-abi": "^4.17.0",
    "wine-darwin": "^1.9.17-1"
  },
  "build": {
    "appId": "com.crispvibe.windsurf.tool",
    "productName": "Windsurf-Tool",
    "copyright": "Copyright © 2024 crispvibe",
    "asar": false,
    "compression": "normal",
    "buildDependenciesFromSource": false,
    "nodeGypRebuild": false,
    "directories": {
      "output": "dist",
      "buildResources": "build"
    },
    "files": [
      "main.js",
      "index.html",
      "language-selector.html",
      "renderer.js",
      "js/**/*",
      "src/**/*",
      "bin/**/*",
      "scripts/**/*",
      "package.json",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}"
    ],
    "extraResources": [
      {
        "from": "scripts",
        "to": "scripts",
        "filter": [
          "**/*.py",
          "**/*.txt",
          "**/*.md"
        ]
      }
    ],
    "afterPack": "./build-scripts/afterPack.js",
    "mac": {
      "category": "public.app-category.developer-tools",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "timestamp": false,
      "identity": null,
      "icon": "build/icon.png",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "artifactName": "${productName}-${version}-macOS-${arch}.${ext}",
      "extraResources": [
        {
          "from": "scripts",
          "to": "scripts",
          "filter": [
            "**/*.py",
            "**/*.txt",
            "**/*.md"
          ]
        }
      ],
      "entitlements": null,
      "entitlementsInherit": null,
      "type": "distribution",
      "minimumSystemVersion": "10.13.0",
      "darkModeSupport": true,
      "mergeASARs": false
    },
    "dmg": {
      "title": "${productName} ${version}",
      "icon": "build/icon.png",
      "background": null,
      "window": {
        "width": 600,
        "height": 450
      },
      "contents": [
        {
          "x": 150,
          "y": 250,
          "type": "file"
        },
        {
          "x": 450,
          "y": 250,
          "type": "link",
          "path": "/Applications"
        }
      ],
      "format": "ULFO",
      "internetEnabled": false
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        {
          "target": "portable",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "icon": "build/icon.ico",
      "artifactName": "${productName}-${version}-${os}-${arch}.${ext}",
      "requestedExecutionLevel": "requireAdministrator",
      "verifyUpdateCodeSignature": false,
      "certificateFile": null,
      "certificatePassword": null,
      "sign": null,
      "extraResources": [
        {
          "from": "scripts",
          "to": "scripts",
          "filter": [
            "**/*.py",
            "**/*.txt",
            "**/*.md"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "perMachine": true,
      "deleteAppDataOnUninstall": false,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Windsurf-Tool",
      "installerIcon": "build/icon.ico",
      "uninstallerIcon": "build/icon.ico",
      "installerHeaderIcon": "build/icon.ico",
      "license": "LICENSE",
      "artifactName": "${productName}-Setup-${version}.${ext}",
      "displayLanguageSelector": false,
      "installerLanguages": [
        "zh_CN",
        "en_US"
      ],
      "language": "2052"
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "deb",
          "arch": [
            "x64"
          ]
        }
      ],
      "category": "Development",
      "icon": "build/icon.png",
      "artifactName": "${productName}-${version}-${arch}.${ext}",
      "desktop": {
        "Name": "Windsurf-Tool",
        "Comment": "Windsurf账号管理工具",
        "GenericName": "Account Manager",
        "Categories": "Development;Utility;"
      }
    }
  }
}
